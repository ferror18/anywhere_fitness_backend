{
  "name": "anywhere_fitness_backend",
  "version": "1.0.0",
  "description": "Backend for the anywhere_fitness app",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run migSeed && node server.js",
    "dev": "nodemon -r dotenv/config server.js",
    "resetdb": "for i in {1..3}; do knex migrate:rollback; done && knex migrate:latest && knex seed:run basic",
    "migSeed": "knex migrate:latest && knex seed:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ferror18/anywhere_fitness_backend.git"
  },
