    "url": "https://github.com/ferror18/anywhere_fitness_backend/issues"
  },
  "homepage": "https://github.com/ferror18/anywhere_fitness_backend#readme",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "express": "4.17.1",
    "faker": "5.1.0",
    "helmet": "4.4.1",
    "jsonwebtoken": "8.5.1",
    "knex": "0.21.16",
    "node": "15.4.0",
    "pg": "8.5.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
